<script> 
	$(document).ready(function(){
		
		$( '#btn' ).on('click',function(){
			$('#btn').on('click',function() {
				$.ajax({
					url:'',
					type:'get',
					dataType: 'text',
					success: function() {
						console.log('성공');
					},
					error: function() {
						console.log( '실패' );
					}
				});
			});
				
			}
			alert( 'btn 클릭' );
		});
	});
</script>
